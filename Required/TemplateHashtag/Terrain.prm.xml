<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<Terrain.prm.xml>

  <Common>
    <Category>
      <Basic>
        <Param>
          <g_FixView>false</g_FixView>
        </Param>
      </Basic>
    </Category>
  </Common>

  <Culling>
    <Category>
      <Basic>
        <Param>
          <g_ExecuteFrustumCulling>true</g_ExecuteFrustumCulling>
          <g_ExecuteOcclusionCulling>true</g_ExecuteOcclusionCulling>
        </Param>
      </Basic>
    </Category>
  </Culling>

  <DynamicLoad>
    <Category>
      <Common>
        <Param>
          <g_GuidePathOffset>0</g_GuidePathOffset>
          <g_ReferenceIntervalUnit>5</g_ReferenceIntervalUnit>
          <g_UseGuidePath>true</g_UseGuidePath>
        </Param>
      </Common>
      <GI-Texture>
        <Param>
          <MaxCreateGetDataCount>50</MaxCreateGetDataCount>
          <g_DummyTextureTypeNotFound>5</g_DummyTextureTypeNotFound>
          <g_DummyTextureTypeNotLoaded>5</g_DummyTextureTypeNotLoaded>
          <g_GITextureLimitSizeDefault>23</g_GITextureLimitSizeDefault>
          <g_GITextureLimitSizePs3>-1</g_GITextureLimitSizePs3>
          <g_GITextureLimitSizeWii>-1</g_GITextureLimitSizeWii>
          <g_GITextureLimitSizeWin32>-1</g_GITextureLimitSizeWin32>
          <g_GITextureLimitSizeXbox360>-1</g_GITextureLimitSizeXbox360>
          <g_GITextureLoadRange>1200</g_GITextureLoadRange>
          <g_Level2Offset>1100</g_Level2Offset>
          <g_VisualizeLoadedLevel>false</g_VisualizeLoadedLevel>
        </Param>
      </GI-Texture>
      <Terrain-Model>
        <Param>
          <g_DynHyterisisDistance>15</g_DynHyterisisDistance>
          <g_RefLookatPositionDistance>100</g_RefLookatPositionDistance>
          <g_TerrainModelLimitSizeDefault>30</g_TerrainModelLimitSizeDefault>
          <g_TerrainModelLimitSizePs3>29</g_TerrainModelLimitSizePs3>
          <g_TerrainModelLimitSizeWii>-1</g_TerrainModelLimitSizeWii>
          <g_TerrainModelLimitSizeWin32>60</g_TerrainModelLimitSizeWin32>
          <g_TerrainModelLimitSizeXbox360>40</g_TerrainModelLimitSizeXbox360>
          <g_TerrainModelLoadRange>2000</g_TerrainModelLoadRange>
        </Param>
      </Terrain-Model>
    </Category>
  </DynamicLoad>

  <DynamicLoadOnStageStart>
    <Category>
      <Common>
        <Param>
          <g_IsUseParamOnStageStart>true</g_IsUseParamOnStageStart>
        </Param>
      </Common>
      <GI-Texture>
        <Param>
          <g_GITextureLimitSizeDefault>20</g_GITextureLimitSizeDefault>
          <g_GITextureLimitSizePs3>-1</g_GITextureLimitSizePs3>
          <g_GITextureLimitSizeWii>-1</g_GITextureLimitSizeWii>
          <g_GITextureLimitSizeWin32>-1</g_GITextureLimitSizeWin32>
          <g_GITextureLimitSizeXbox360>-1</g_GITextureLimitSizeXbox360>
        </Param>
      </GI-Texture>
      <Terrain-Model>
        <Param>
          <g_TerrainModelLimitSizeDefault>20</g_TerrainModelLimitSizeDefault>
          <g_TerrainModelLimitSizePs3>19</g_TerrainModelLimitSizePs3>
          <g_TerrainModelLimitSizeWii>-1</g_TerrainModelLimitSizeWii>
          <g_TerrainModelLimitSizeWin32>60</g_TerrainModelLimitSizeWin32>
          <g_TerrainModelLimitSizeXbox360>20</g_TerrainModelLimitSizeXbox360>
        </Param>
      </Terrain-Model>
    </Category>
  </DynamicLoadOnStageStart>

</Terrain.prm.xml>
